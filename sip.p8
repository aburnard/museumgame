--animation code

function toggle_tiles()
	for x=0,56 do
	 for y=0,56 do
	  if (is_tile(anim1,x,y)) then
	   swap_tile(x,y)
	   
	  elseif (is_tile(anim2,x,y)) then
	   unswap_tile(x,y)
	   
	  end
	 end
	end
   end



function swap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile+1)
end
   
function unswap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile-1)
end

