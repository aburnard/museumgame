--map code

function map_setup()
--timers
timer=0
anim_time=10 --30 = 1 second
--map tile settings
mapx=0
mapy=0
wall=0
lady=1
anim1=2
anim2=3
dlscal=4
leftscalr=6
escalr=7
mapbias=128
edir=1 --escalator direction
end

function update_map()
	if (timer<0) then
		toggle_tiles()
		timer=anim_time
		
	end
	timer-=1
	roomx=mapx/mapbias
	roomy=mapy/mapbias
end

function draw_map()
 
 mapx=flr(px/mapbias)*mapbias
 mapy=flr(py/mapbias)*mapbias
 camera(mapx,mapy)
	map(0,0,0,0,128,64)
	
end

function draw_topmap()

 if (roomx==0 and roomy==1) then 
 camera(0,0)
 map(112,18,0,0,128,64)
	end
end



function is_tile(tile_type,x,y)
	tile=mget(x,y)
	has_flag=fget(tile,tile_type)
	return has_flag
end

function can_move(x,y)
	return not is_tile(wall,x,y)

end

   
function escalater(x,y)
 return is_tile(escalr,x,y)
end


function leftescal(x,y)
 return is_tile(leftscalr, x,y)
end

function mum()
	mum=true
end


