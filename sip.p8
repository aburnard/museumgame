version 30
__lua__
--game loop
--find out how I check collision and use that
--to determine if a square matches escalator
function _init()
	escalator_speed=0
	map_setup()
 speech_bubble=""
	--bubble_time=0
	random_encounter=false
	timing_loop=30--speach bubble
	t_loop=0
 make_player()
end

function _update()
  b0=btn(0) --left state
  b1=btn(1) --right state
  b2=btn(2) --up state
  b3=btn(3) --down'
  t_loop+=1
  if (t_loop == escalator_speed + 1) t_loop=0
  pat+=1 --increment state clock
  check_escalators()
  update_speachtime()
  if (pstate==0) idle_state()
  if (pstate==1) walking_state()
  if (pstate==2) escalate()
  if (pstate==3) descalate()
--escape random encounter
  if (btnp(❎))random_encounter=false

  encounter() 
  update_map()
end

function _draw()
  cls()
  draw_map()
  draw_player()
  --draw_topmap()
  if(random_encounter) dialogue_text()

  if(speech_bubble!="") then
    if(bubble_time > 0) then
	     message_popup()
	   end
  end
  display_debug()
end



