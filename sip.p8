--state code
--changestate

function change_state(s)
 pstate=s
 pat=0
end
function idle_state()
 pspr=69
if (b0 or b1 or b2 or b3)change_state(1)
end

--walking state
function walking_state()
   --left
   if (b0) then
   pdir=-1
   pupdown=0 
   
   
   end
   --right
   if (b1) then
   pdir=1	
   pupdown=0
   end
   --up
   if (b2) then 
   pupdown=-1
   pdir=0
   end
   --down
   if (b3) then
   pupdown=1
   pdir=0
   end
   if not can_move((px + pdir/8 )/8,((py + pupdown+8)/8)) or not can_move((px + 8 + pdir/8 )/8,((py + pupdown+8)/8)) or not can_move((px + 8 + pdir/8 )/8,((py + pupdown+15)/8)) or not can_move((px +  pdir/8 )/8,((py + pupdown+15)/8)) then
	pupdown=0
  pdir=0
  end
  
   px+=pdir*2
   py+=pupdown*2
   
   pspr=flr(pat/4)%4+69
   if (not(b0 or b1 or b2 or b3)) change_state(0)
    

end


--escalator state
 function escalate() 	

	if t_loop == escalator_speed then
	
	py-=8
	
	end
	if not(escalater(px,((py + pupdown+8)/8))) then
	change_state(0)
	end
end
--down escalator state
function descalate()
   
	
	py+=8
	
	if not(escalater((px),((py + pupdown+8)/8))) then
	change_state(0)
	end
end

function check_escalators()
if  escalater((px + pdir/8 )/8,((py + pupdown+8)/8)) then
	sfx(0)
	change_state(2)
end

--left esc state  
if  leftescal((px + pdir/8 )/8,((py + pupdown+8)/8)) then
	sfx(0)
	change_state(3)
end
end

--randomencounters

function encounter()
if (btnp(⬅️) or btnp(➡️)) then
	if (rnd(200)>199) then
		random_encounter=true
	end
	end
end
